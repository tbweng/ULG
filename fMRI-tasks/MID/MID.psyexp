        <Param name="Begin Routine" updates="constant" val="msgEarnings = &quot;You earned a total of: $%.2f!&quot; % TrackEarnings&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
